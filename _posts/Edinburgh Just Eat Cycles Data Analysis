    "# Create function to convert metres/second to miles per hour\n",
    "def mph_speed(d, t):\n",
    "    ''' Create function to calculate speed (MPH) from distance (d) and time (t)'''\n",
    "    s = d / t\n",
    "    return 2.23694 * s\n",
    "\n",
    "# Iterate over the rows in 'df_1' to create new columns to calculate speed for each trip\n",
    "for index, row in df_1.iterrows():\n",
    "        df_1['day'] = started_at.dt.dayofweek\n",
    "        df_1['distance'] = trip_distance\n",
    "        df_1['speed'] = mph_speed(df_1['distance'], df_1['duration'])\n",
    "\n",
    "# Create array of the speed column\n",
    "speed = df_1.as_matrix(columns=['speed'])\n"
   ]
  },
